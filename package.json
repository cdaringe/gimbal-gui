{
  "name": "gimbal_gui",
  "version": "1.0.0",
  "scripts": {
    "build:app": "gleam format && gleam build",
    "build:css": "unocss --config ./unocss.config.ts **/*.gleam src/**/*.gleam index.html",
    "build": "run-p build:*",
    "start:server": "httpster .",
    "start:unocss": "run-s 'build:css --watch'",
    "start:gleam": "npm run build && chokidar '**/*.gleam' -c 'npm run build:app'",
    "start": "run-p start:*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@unocss/cli": "^0.58.3",
    "chokidar-cli": "3.0.0",
    "httpster": "1.2.1",
    "npm-run-all": "4.1.5",
    "unocss": "^0.58.3"
  }
}
